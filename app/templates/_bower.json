{
    "name": "<%= _.slugify(appName) %>",
    "version": "0.0.0",<% if (validGit){ %>
    "homepage": "<%= git %>/<%= _.slugify(gitUser) %>/<%= _.slugify(appName) %>",
    <% } %><% if (gitUser){ %>"authors": [
        "<%= _.slugify(gitUser) %>"
    ],<% } %>
    "description": "<%= appDescription %>",
    "main": "dist/js/<%= _.slugify(appName) %>.js",
    "keywords": [
        "javascript",
        "canjs",
        "handlebars"
    ],
    "license": "MIT",
    "ignore": [
        ".*/",
        "**/.*",
        "node_modules",
        "bower_components",
        "test",
        "tests"
    ],
    "dependencies": {
        "almond": "~0.2.7",
        "normalize-css": "~2.1.3",
        "canjs": "~2.0.4",
        "jquery": "~2.0.3",
        "requirejs-i18n": "~2.0.4",
        "requirejs-text": "~2.0.10"<% if (installFlexBoxGrid){ %>,
        "flexboxgrid": "https://github.com/kristoferjoseph/flexboxgrid.git#~0.1.0"<% } %>
    }
}


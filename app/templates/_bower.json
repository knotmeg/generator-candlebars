{
    "name": "<%= appName %>",
    "version": "0.0.0",
    "homepage": "<%= gitHome %>",
    "authors": [
        "<%= gitUser %>"
    ],
    "description": "<%= appDescription %>",
    "main": "dist/js/<%= appName %>.js",
    "keywords": [
